# -*- coding: utf-8 -*-

import numpy as np
import random

def printRand():
  b = random.randint(1, 10)
  c = random.randint(1, 10)
  a = np.random.random_integers(0, 1, (b, c))
  print "%d %d" % (c, b) 
  for r in a:
    print " ".join(map(str, r.tolist()))

if __name__ == "__main__":
  printRand()

